<html>
    <head>
        <link href="stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" />
        <link href="stylesheets/print.css" media="print" rel="stylesheet" type="text/css" />
        <!--[if lt IE 8]>
            <link href="stylesheets/ie.css" media="screen, projection" rel="stylesheet" type="text/css" />
        <![endif]-->
    <script src="js/jquery-1.9.0/jquery-1.9.0.min.js"></script>
    <script src="js/restapidoc.js"></script>

    </head>
    <body class="bp two-col">
        <div id="container">

            <div id="header">
                <h1>NextWab REST API v0.1.0</h1>
            </div> <!-- End of header -->

            <div id="sidebar">
                <div id="servicesMenu">
                    <div class="block-title">Services:</div>
                    <ul>
                        <li><a href="Service-1">Service-1</a></li>
                        <li><a href="Service-2">Service-2</a></li>
                        <li><a href="Service-3">Service-3</a></li>
                        <li><a href="Service-4">Service-4</a></li>
                    </ul>
                </div>
            </div> <!-- End of sidebar -->

            <div id="content">

                <div class="serviceNameBox">
                    <h2>{{=it.name}}</h2>
                </div>

                <p>{{=it.description}}</p>

                <div class="block-title"><h3>Methods:</h3></div>
                <div id="methods">
                {{ for(var method in it.methods) { }}

                    <div id="methodHeader">
                        <div class="methodNameBox">
                            <div class="method">{{=method}}</div>
                        </div>

                        <div class="urlPatternBox">
                            <div class="urlPatterns">{{=it.urlPattern}}</div>
                        </div>
                            <p>{{=it.methods[method].summary}}</p>
                            <p>{{=it.methods[method].details}}</p>
                    </div>

                    <div class="block-title"><h4>Request:</4></div>
                    <div id="request">
                    {{ { var request = it.methods[method].request; } }}

                        <div class="block-title"><h5>Parameters:</h5></div>
                        <div id="parameters">
                            <table>
                                <tr>
                                    <th>Parameter</th>
                                    <th>kind</th>
                                    <th>type</th>
                                    <th>default</th>
                                    <th>required</th>
                                    <th>summary</th>
                                </tr>
                                {{~ request.parameters :param:pidx}}
                                <tr>
                                    <td>{{=param.name}}</td>
                                    <td>{{=param.kind}}</td>
                                    <td>{{=param.type}}</td>
                                    <td>{{=param.default}}</td>
                                    <td>{{=param.required}}</td>
                                    <td>{{=param.summary}}</td>
                                </tr>{{~}}
                            </table>
                        </div>

                        <div class="block-title"><h5>Headers:</h5></div>
                        <div id="headers">
                        <table>
                            <tr>
                                <th>Field name</th>
                                <th>Value</th>
                            </tr>
                        {{ for (var header in request.headers) { }}
                            <tr>
                                <td>{{=header}}</td>
                                <td>{{=request.headers[header]}}</td>
                            </tr>
                        {{ } }}
                        </table>
                        </div>

                        <div class="block-title"><h5>Cookies:</h5></div>
                        <div id="cookies">
                        <table>
                            <tr>
                                <th>Field name</th>
                                <th>Value</th>
                            </tr>
                        {{ for (var cookie in request.cookies) { }}
                            <tr>
                                <td>{{=cookie}}</td>
                                <td>{{=request.cookies[cookie]}}</td>
                            </tr>
                        {{ } }}
                        </table>
                        </div>

                    </div> <!-- End of Request -->

                    <div class="block-title"><h4>Responses:</h4></div>
                    <div id="responses">
                    {{ for (var responseId in it.methods[method].responses) { }}
                        {{ var response = it.methods[method].responses[responseId]; }}
                        <div id="response">

                            <div class="block-title">
                                <h5>{{=response.statusCode}} - {{=response.reason}}</h5>
                            </div>
                            <p>Response validation schema: <a href="{{=response.validationSchema}}">{{=response.validationSchema}}</a></p>

                            <div class="block-title"><h5>Headers:</h5></div>
                            <div id="headers">
                            <table>
                                <tr>
                                    <th>Field name</th>
                                    <th>Value</th>
                                </tr>
                            {{ for (var header in response.headers) { }}
                                <tr>
                                    <td>{{=header}}</td>
                                    <td>{{=response.headers[header]}}</td>
                                </tr>
                            {{ } }}
                            </table>
                            </div>

                            <div class="block-title"><h5>Cookies:</h5></div>
                            <div id="cookies">
                            <table>
                                <tr>
                                    <th>Field name</th>
                                    <th>Value</th>
                                </tr>
                            {{ for (var cookie in response.cookies) { }}
                                <tr>
                                    <td>{{=cookie}}</td>
                                    <td>{{=response.cookies[cookie]}}</td>
                                </tr>
                            {{ } }}
                            </table>
                            </div>

                            <div class="block-title"><h5>Examples:</h5></div>
                            <div id="examples">
                            </div>

                            </div>
                        </div> <!-- End of Response -->

                    {{ } }}
                    </div> <!-- End of Responses -->

                {{ } }}
                <!div> <!-- End of methods -->

            </div> <!-- End of content -->

            <div id="footer">
                Generated by RestApi. Author TomBenke
            </div> <!-- End of footer -->

        </div> <!-- End of container -->
    </body>
</html>